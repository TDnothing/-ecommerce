.panel-lite {
  margin: 20px auto;
  max-width: 360px;
  background: #fff;
  padding: 35px 20px;
  border-radius: 4px;
  box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.2);
  position: relative;
  margin-top: 150px;
}
.panel-lite h4 {
  font-weight: 400;
  font-size: 24px;
  text-align: center;
  color: #27ae61;
  margin: 15px auto;
}
.panel-lite .go{text-align: center; margin-top: 20px;}
.panel-lite a {
  display: inline-block;
  margin-top: 25px;
  text-decoration: none;
  color: #27ae61;
  font-size: 14px;
  margin: auto;
}
.panel-lite .form-group {
  position: relative;
  font-size: 15px;
  color: #666;
}
.panel-lite .form-group + .form-group {
  margin-top: 30px;
}
.panel-lite .form-group .form-label {
  position: absolute;
  z-index: 1;
  left: 0;
  top: 5px;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  font-weight: normal;
}
.panel-lite .form-group .form-control {
  width: 100%;
  position: relative;
  z-index: 3;
  height: 35px;
  background: none;
  border: none;
  padding: 5px 0;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  border-bottom: 1px solid #777;
  border-radius: 0px;
  -webkit-box-shadow: inset 0 0px 0px rgba(0,0,0,.075);
  box-shadow: inset 0 0px 0px rgba(0,0,0,.075);
  /* -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s; */
}
.panel-lite .form-group city{
	position: absolute; right: 30px; top:0px;
}
.panel-lite .form-group .form-control:invalid {
  outline: none;
}
.panel-lite .form-group .form-control:focus, .panel-lite .form-group .form-control:valid {
  outline: none;
  color: #27ae61;
  box-shadow: 0 1px #27ae61;
  border-color: #27ae61;
}
.panel-lite .form-group .form-control:focus + .form-label, .panel-lite .form-group .form-control:valid + .form-label {
  font-size: 12px;
  -ms-transform: translateY(-15px);
  -webkit-transform: translateY(-15px);
  transform: translateY(-15px);
}

.floating-btn {
  background: #27ae61;
  width: 60px;
  height: 60px;
  border-radius: 50%;
  color: #fff;
  font-size: 32px;
  border: none;
  position: absolute;
  margin: auto;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  box-shadow: 1px 0px 0px rgba(0, 0, 0, 0.3) inset;
  margin: auto;
  right: -30px;
  bottom: 82px;
  cursor: pointer;
}
.floating-btn:hover {
  box-shadow: 0 0 0 rgba(0, 0, 0, 0.3) inset, 0 3px 6px rgba(0, 0, 0, 0.16), 0 5px 11px rgba(0, 0, 0, 0.23);
}
.floating-btn:hover .icon-arrow {
  -ms-transform: rotate(45deg) scale(1.2);
  -webkit-transform: rotate(45deg) scale(1.2);
  transform: rotate(45deg) scale(1.2);
}
.floating-btn:focus, .floating-btn:active {
  outline: none;
}

.icon-arrow {
  position: relative;
  width: 13px;
  height: 13px;
  border-right: 3px solid #fff;
  border-top: 3px solid #fff;
  display: block;
  -ms-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  margin: auto;
  -webkit-transition: 0.3s;
  transition: 0.3s;
}
.glyphicon-refresh{
	position: relative;
  font-size:18px;
  display: block;
  margin: auto;
  -webkit-transition: 0.3s;
  transition: 0.3s;
}
.icon-arrow:after {
  content: '';
  position: absolute;
  width: 18px;
  height: 3px;
  background: #fff;
  left: -5px;
  top: 5px;
  -ms-transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
}



