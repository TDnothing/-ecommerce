/* 我们的服务 */
.f-service-w{background-color: #F5F5F5; }
.f-service{ padding:0px 10px; margin-bottom: 40px; position: relative;}
.f-service .item{width:100%;height:300px;float:left;position:relative;cursor:pointer}
.f-service .item_o{width:160px;height:160px;border-radius:50%;border:2px solid #f8f8f8;top:75px;position:absolute;opacity:0;left:50%; margin-left: -80px;}
.f-service .item_g{width:128px;height:128px;border-radius:50%;border:2px solid #27ae61;margin:90px auto 0;}
.f-service .item_w{width:124px;height:124px;border-radius:50%;border:6px solid #f8f8f8;transition:all 1s; text-align: center;
	background:#27ae61;border-color:#fff}
.f-service .item_w i{font-size:34px;line-height:116px;transition:all 1s; color: #FFFFFF;}
.f-service .item p{color:#2d3e50;font-size:20px;margin-top:30px;display:block;transition:all 1s; text-align: center;}
.f-service .item p i{position:relative;top:0px;transition:all 1s;}
.f-service .item:hover .item_o{border-color:#ffce55;-webkit-animation:shan 1.5s;animation:shan 1.5s;}
	@-webkit-keyframes shan{ 1%{opacity:1} 100%{width:180px;height:180px;top:65px;left:50%; margin-left: -90px;opacity:0;} }
	@keyframes shan{ 1%{opacity:1} 100%{width:180px;height:180px;top:65px;left:50%; margin-left: -90px;opacity:0;} }
.f-service .item:hover .item_w{background:#27ae61;border-color:#fff}
.f-service .item:hover p{color:#27ae61;}
.f-service .item:hover .item_w i{color:#fff}
.f-service .item:hover p i{color:#27ae61;margin:0px 5px 0px 5px; position: relative; top: 0px;}

