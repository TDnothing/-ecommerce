@using Domain.Entity
@model tb_Site
@{
    ViewBag.Title = "关于飞易腾_" + Model.SeoTitle;
    ViewBag.Key = Model.SeoKey;
    ViewBag.Desc = Model.SeoDescribe;
    ViewBag.Nav = "4";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section scripts
{
    <script type="text/javascript" src="/lib/js/about.js"></script>
}
